{
    "name": "abovesky/dcat-lightbox",
    "description": "Dcat Admin 图片预览扩展",
    "type": "library",
    "keywords": ["dcat-admin", "extension", "lightbox"],
    "homepage": "https://github.com/dcat-admin-extensions/lightbox",
    "license": "MIT",
    "authors": [
        {
            "name": "abovesky",
            "email": "abovesky@qq.com"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "dcat/laravel-admin": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "Abovesky\\DcatAdmin\\Lightbox\\": "src/"
        }
    },
    "extra": {
        "dcat-admin": "Abovesky\\DcatAdmin\\Lightbox\\LightboxServiceProvider",
        "laravel": {
            "providers": [
                "Abovesky\\DcatAdmin\\Lightbox\\LightboxServiceProvider"
            ]
        }
    }
}
